{
  "name": "ai-prd-generator",
  "displayName": "Mise-en-Place",
  "description": "AI-Powered PRD Generator",
  "version": "0.0.8",
  "publisher": "jammie-anson",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/johndoe/ai-prd-generator.git"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ai-prd-generator.generatePrd-dev",
        "title": "Mise-en-Place: Generate PRD"
      }
    ],
    "configuration": {
      "title": "AI PRD Generator",
      "properties": {

      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "node esbuild.js",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vscode-test",
    "test:analyzer": "ts-node src/context-card-generator/analyzer.ts"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/markdown-it": "^14.1.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.89.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.8",
    "eslint": "^9.25.1",
    "fs-extra": "^11.3.0",
    "markdown-it": "^14.1.0",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@types/cytoscape": "^3.21.9",
    "@types/node-fetch": "^2.6.12",
    "cytoscape": "^3.32.1",
    "json-viewer-js": "^1.0.8",
    "node-fetch": "^2.7.0",
    "openai": "^5.10.1",
    "tree-sitter": "^0.21.0",
    "tree-sitter-typescript": "^0.23.2"
  }
}
